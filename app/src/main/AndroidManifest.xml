<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sananhaji.androidders">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidDers">
        <activity android:name=".ui.activity.viewpager.DateTimeActivity"></activity>
        <activity android:name=".ui.activity.viewpager.WebViewActivity" />
        <activity android:name=".ui.activity.viewpager.WidgetsActivity" />
        <activity android:name=".ui.activity.viewpager.ViewPagerActivity" />
        <activity android:name=".ui.activity.TestActivity" />
        <activity android:name=".ui.activity.CalculatorActivity" />
        <activity android:name=".ui.activity.StudentListActivity" />
        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>